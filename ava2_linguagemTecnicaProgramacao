#include <stdio.h>

int main() {
    int matriz[3][3] = {{2, 4, 6},
                        {8, 10, 12},
                        {14, 16, 18}};

    printf("\nMatriz original: \n");

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            printf("%d ", matriz[i][j]);
        }
        printf("\n");
    }

    printf("\nMatriz com elementos multiplicados por 5:\n");

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            printf("%d ", (matriz[i][j]) * 5);
        }
        printf("\n");
    }
}
